1: ADDITION OF TWO 8-BIT NUMBERS
		MOV A, #DATA		
		MOV B, #DATA		
		ADD A, B		
		MOV R0, A		

Program 2: SUBTRACTION OF TWO 8-BIT NUMBERS
		MOV A, #DATA		
		MOV B, #DATA		
		SUBB A, B		
		MOV R0, A		

Program 3: MULTIPLICATION OF TWO 8-BIT NUMBERS
		MOV A, #DATA		
		MOV B, #DATA		
		MUL AB		
		MOV R0, A		
		MOV R1, B		

4.DIVISION OF TWO 8-BIT NUMBERS
		MOV A, #DATA		
		MOV B, #DATA		
		DIV AB		
		MOV R0, A		
		MOV R1, B		

5.AND OPERATION OF TWO 8-BIT NUMBERS
		MOV A, #DATA		
		MOV B, #DATA		
		ANL A, B		
		MOV R0, A		

Program 6: OR OPERATION OF TWO 8-BIT NUMBERS
		MOV A, #DATA		
		MOV B, #DATA		
		ORL A, B		
		MOV R0, A		

Program 7: ONE'S COMPLEMENT/INVERSION
		MOV A, #DATA		
		CPL A		
		MOV R0, A		

Program 8: TWOâ€™S COMPLEMENT OPERATION
		MOV A, #01H		
		CPL A		
		MOV 30H, A		
		INC A		
		MOV 31H, A		
LOOP	SJMP LOOP		

